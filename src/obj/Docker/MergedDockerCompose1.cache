services:
  booking.api:
    build:
      context: E:\WorkSpace\microservices\Activities_microservice\src
      dockerfile: E:\WorkSpace\microservices\Activities_microservice\src\Services\Bookings\Booking.API\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Booking.API
      target: base
    container_name: booking.api
    depends_on:
      bookingDb:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      DatabaseSettings:ConnectionString: mongodb://bookingDb:27017
      NUGET_FALLBACK_PACKAGES: ""
    image: bookingapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Booking.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: 6001
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: E:\WorkSpace\microservices\Activities_microservice\src\Services\Bookings\Booking.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: E:\WorkSpace\microservices\Activities_microservice\src
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\qbuser\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\qbuser\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\qbuser\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\qbuser\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  bookingDb:
    container_name: bookingDb
    image: mongo
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: 27017
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: mongo_data
      target: /data/db
      volume: {}
networks:
  default:
    name: dockercompose1716024832730829480_default
volumes:
  mongo_data:
    name: dockercompose1716024832730829480_mongo_data